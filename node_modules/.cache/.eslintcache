itals.js",[],"/Applications/Developer/Projects/reactsearchappyelp/src/components/App/App.js",[],"/Applications/Developer/Projects/reactsearchappyelp/src/components/Business/Business.js",["61","62","63"],"import React, { Component } from 'react';\nimport ReactDOM from 'react-dom';\nimport './Business.css';\n\nclass Business extends React.Component {\n    render(){\n        return(\n            <div className=\"Business\">\n            <div className=\"image-container\">\n            <img src='https://s3.amazonaws.com/codecademy-content/programs/react/ravenous/pizza.jpg' alt=''/>\n            </div>\n            <h2>{this.props.business.name}</h2>\n            <div className=\"Business-information\">\n                <div className=\"Business-address\">\n                <p>{this.props.business.address}</p>\n                <p>{this.props.business.city}</p>\n                <p>{this.props.business.state, this.props.business.zipCode}</p>\n                </div>\n                <div className=\"Business-reviews\">\n                <h3>{this.props.business.category}</h3>\n                <h3 className=\"rating\">{this.props.business.rating}</h3>\n                <p>{this.props.business.reviewCount}</p>\n                </div>\n            </div>\n            </div>\n        )\n    }\n}\n\nexport default Business;\n\n\n","/Applications/Developer/Projects/reactsearchappyelp/src/components/BusinessList/BusinessList.js",[],"/Applications/Developer/Projects/reactsearchappyelp/src/components/SearchBar/SearchBar.js",["64"],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/index.js",[],["65","66"],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/reportWebVitals.js",[],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/components/App/App.js",[],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/components/BusinessList/BusinessList.js",[],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/components/SearchBar/SearchBar.js",["67"],"import React from 'react';\nimport './SearchBar.css'\n\n\n\nclass SearchBar extends React.Component {\n\n    constructor(props){\n        super(props)\n        //Setting up the initial state of the search bar. Setting up the obj. state: temr & location are set up to empty strings. Each key value will be changed with \n        //my own methods written below. See handleTermChange & handleLocationChange & \n        this.state = {\n            term : '',\n            location : '',\n            sortBy : 'best_match'\n        };\n        this.sortByOptions = {\n            'Best Match' : 'best_match',\n            'Highest Rated' : 'rating',\n            'Most Reviewed' : 'review_count'\n        }\n        //Binding the methods for the event listeners. I MUST bind a method to an event handler.\n        this.handleTermChange = this.handleTermChange.bind(this);\n        this.handleLocationChange = this.handleLocationChange.bind(this);\n        this.handleSearch = this.handleSearch.bind(this)\n    }\n\n    //Handling Search when a user click on the button (onClick event)\n\n    handleSearch(event){\n        event.preventDefault()\n        this.props.searchYelp(this.state.term,this.state.location,this.state.sortBy)\n        \n        \n    }\n    //This one is giving the user a visual feedback.\n    getSortByClass(sortByOption) {\n        if (this.state.sortBy === sortByOption) {\n          return 'active';\n        }\n        return '';\n      }\n    \n    \n    //These three methods bleow will be changign the state with .setState \n    handleSortByChange(sortByOption){\n        this.setState({\n            sortBy : sortByOption\n        })\n        \n    }\n\n    handleTermChange(event){\n        this.setState({\n            term : event.target.value \n        })\n\n    }\n\n    handleLocationChange(event){\n        this.setState({\n            location : event.target.value\n        })\n\n\n    }\n\n    // This method will render an <li> element which will be placed inside <ul> in return().\n    renderSortByOptions(){\n        return(Object.keys(this.sortByOptions)).map(sortByOption => {\n\n            let sortByOptionValue = this.sortByOptions[sortByOption];\n\n            return(<li key={sortByOptionValue} onClick={this.handleSortByChange.bind(this,sortByOptionValue)} className={this.getSortByClass(sortByOptionValue)}>{sortByOption}</li>)\n\n        });\n    }\n    //Now create the structure fo my SearchBar Component.\n    render(){\n        return(\n            <div className=\"SearchBar\">\n            <div className=\"SearchBar-sort-options\">\n                <ul>\n                {this.renderSortByOptions()}\n                </ul>\n            </div>\n            <div className=\"SearchBar-fields\">\n                <input placeholder=\"Search Businesses\" onChange={this.handleTermChange}/>\n                <input placeholder=\"Where?\" onChange={this.handleLocationChange}/>\n            </div>\n            <div className=\"SearchBar-submit\">\n                <a onClick={this.handleSearch}>Let's Go</a>\n            </div>\n            </div>\n        )\n    }\n}\n\n\n\nexport default SearchBar;","/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/components/Business/Business.js",["68","69"],"/Applications/Developer/GitCracken/react-foodapp-using-yeplapi/src/util/Yelp.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":1,"column":17,"nodeType":"76","messageId":"77","endLine":1,"endColumn":26},{"ruleId":"74","severity":1,"message":"78","line":2,"column":8,"nodeType":"76","messageId":"77","endLine":2,"endColumn":16},{"ruleId":"79","severity":1,"message":"80","line":17,"column":46,"nodeType":"81","messageId":"82","endLine":17,"endColumn":47},{"ruleId":"83","severity":1,"message":"84","line":92,"column":17,"nodeType":"85","endLine":92,"endColumn":48},{"ruleId":"70","replacedBy":"86"},{"ruleId":"72","replac